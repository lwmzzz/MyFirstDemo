<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.dc.lwm.dao.StudentStateViewDao">
	<select id="findAll"
		resultType="com.dc.lwm.entity.StudentStateView">
		SELECT *
		FROM studentstateview
	</select>
	<select id="queryByI"
		resultType="com.dc.lwm.entity.StudentStateView">
		SELECT *
		FROM studentstateview
		WHERE stu_id=#{stuId}
	</select>
</mapper>