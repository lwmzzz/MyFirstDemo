<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.dc.lwm.dao.TeacherInfoViewDao">
	<select id="findAll"
		resultType="com.dc.lwm.entity.TeacherInfoView">
		SELECT *
		FROM
		teacherinfoview
	</select>
	<select id="queryById"
		resultType="com.dc.lwm.entity.TeacherInfoView">
		SELECT *
		FROM
		teacherinfoview
		WHERE tea_id=#{teaId}
	</select>
</mapper>