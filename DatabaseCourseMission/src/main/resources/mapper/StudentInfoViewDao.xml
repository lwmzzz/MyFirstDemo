<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.dc.lwm.dao.StudentInfoViewDao">
	<select id="findAll"
		resultType="com.dc.lwm.entity.StudentInfoView">
		SELECT *
		FROM studentinfoview
	</select>
	<select id="queryById"
		resultType="com.dc.lwm.entity.StudentInfoView">
		SELECT *
		FROM studentinfoview
		WHERE stu_id=#{stuId}
	</select>
</mapper>
